{
  "name": "baddriverreport",
  "version": "1.0.0",
  "description": "defining the APIs for bad driver reporting system",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "forever-start": "forever start -l /home/ubuntu/project/log -o /home/ubuntu/project/stdout_log -e /home/ubuntu/project/stderr_log -a server.js --port 9001 --db bdr_db",
    "forever-stop": "forever stopall",
    "forever-start-test": "forever start -l /home/ubuntu/project/test_log -o /home/ubuntu/project/test_stdout_log -e /home/ubuntu/project/test_stderr_log -a server.js --port 9010 --db bdr_db_test",
    "start-test": "node server.js --port 9010 --db bdr_db_test",
    "install-prod": "npm install --only=production"
  },
  "author": "BDR_team",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.1",
    "busboy-body-parser": "^0.3.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "gridfs-stream": "^1.1.1",
    "helmet": "^3.12.0",
    "https": "^1.0.0",
    "jwt-simple": "^0.5.5",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "mongodb": "^2.2.26",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.3",
    "passport": "^0.4.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/cookie-session": "^2.0.34",
    "@types/express": "^4.11.1",
    "@types/gridfs-stream": "^0.5.30",
    "@types/helmet": "0.0.37",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^2.2.19",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.7",
    "@types/nodemailer": "^4.6.0",
    "@types/winston": "^2.3.8",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "typescript": "^2.7.2"
  }
}
